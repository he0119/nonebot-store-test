{
  "name": "nonebot-registry",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:preview": "vue-tsc && vite build --mode preview",
    "preview": "vite preview",
    "eslint": "eslint --cache --report-unused-disable-directives",
    "eslint:fix": "pnpm eslint --fix",
    "stylelint": "stylelint",
    "stylelint:fix": "pnpm stylelint --fix",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:style:fix",
    "lint:js": "pnpm eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:js:fix": "pnpm eslint:fix \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:style": "pnpm stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "pnpm stylelint:fix \"**/*.{css,scss}\"",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@sentry/vue": "^7.113.0",
    "@vueuse/core": "^10.9.0",
    "ansi_up": "^6.0.2",
    "gsap": "^3.12.5",
    "highlight.js": "^11.9.0",
    "naive-ui": "^2.38.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.16.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@unocss/transformer-directives": "^0.59.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.16"
  }
}
