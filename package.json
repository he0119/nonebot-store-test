{
  "name": "nonebot-registry",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint --cache --report-unused-disable-directives",
    "eslint:fix": "pnpm eslint --fix",
    "stylelint": "stylelint",
    "stylelint:fix": "pnpm stylelint --fix",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:style:fix",
    "lint:js": "pnpm eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:js:fix": "pnpm eslint:fix \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:style": "pnpm stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "pnpm stylelint:fix \"**/*.{css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "ansi_up": "^5.2.1",
    "axios": "^1.4.0",
    "highlight.js": "^11.8.0",
    "naive-ui": "^2.34.4",
    "vue": "^3.2.47",
    "vue-material-design-icons": "^5.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  }
}
